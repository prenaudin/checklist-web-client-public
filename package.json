  "scripts": {
    "start": "if [ \"$NODE_ENV\" = \"production\" ]; then npm run serve-prod; else npm run serve-dev; fi",
    "postinstall": "if [ \"$NODE_ENV\" = \"production\" ]; then npm run build; fi",
    "build": "$(npm bin)/webpack --config ./webpack.production.js --progress --cheap-eval-source-map",
    "serve-dev": "NODE_ENV=development $(npm bin)/webpack-dev-server --config ./webpack.development.js --hot --progress --inline --history-api-fallback --cheap-eval-source-map",
    "serve-prod": "NODE_ENV=production node server.prod.js",
    "lint": "npm run eslint",
    "eslint": "eslint --ext .js,.jsx ./src",
    "test": "NODE_ENV=test $(npm bin)/karma start --single-run --no-auto-watch ./karma.config.js",
    "test-watch": "NODE_ENV=test $(npm bin)/karma start ./karma.config.js"
  },
